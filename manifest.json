{
  "developer": {
    "author": "Daniel Young",
    "url": "https://github.com/DanRYoung/twitch-timestamp-tracker"
  },
  "manifest_version": 2,
  "name": "Twitch Timestamp Tracker",
  "version": "1.0",
  "description": "Continually update the Twitch video timestamp URL for sharing",
  "icons": {
    "48": "icons/icon.svg"
  },
  "content_scripts": [
    {
      "matches": ["*://*.twitch.tv/videos/*"],
      "js": ["build/index.js"]
    }
  ]
}
